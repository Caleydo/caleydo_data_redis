{
  "name": "caleydo_data_redis",
  "description": "Data provider plugin for loading data stored in a Redis in-memory database.",
  "version": "0.0.1",
  "license" : "SEE LICENSE IN LICENSE",
  "repository": "Caleydo/caleydo_data_redis",
  "dependencies": {
  },
  "peerDependencies": {
    "caleydo_server": "*"
  },
  "caleydo": {
    "type": "plugin",
    "category": "python_data",
    "plugins": {
      "python": [
        {
          "type": "manager",
          "id": "idmanager",
          "file": "assigner",
          "priority": -5,
          "singleton": true
        },
        {
          "type": "manager",
          "id": "mappingmanager",
          "file": "mapper",
          "priority": -5,
          "singleton": true
        },
        {
          "type": "mapping_table",
          "file": "mapping_table",
          "mappings": [
            "Ensembl2UniProt",
            "UniProt2Ensembl"
          ]
        }
      ]
    },
    "dependencies": {
      "python": {
        "redis": "==2.10.5"
      },
      "debian": {
        "redis-server": ""
      },
      "redhat": {
        "redis": ""
      }
    },
    "scripts": {
      "pre_start": "sudo service redis-server start",
      "pre_start_redhat": "sudo service redis start"
    }
  },
  "publishConfig": {
    "registry": "http://registry.caleydo.org/"
  }
}
